---
host:
  alsa:
    modprobe:
      - options snd-intel-dspcfg dsp_driver=1
  boot:
    network:
      -
        name: wifi
        contents:
          Match:
            Name: wlp0s20f3
          Network:
            DHCP: true
  desktop:
    acpi: intel_backlight
    dpi: 226
  disk:
    dev: /dev/nvme0n1
    prefix: p
  postgresql:
    cow: true
  system:
    modprobe:
      - options i915 modeset=1
      - options i915 enable_psr=0
  user:
    autologin: true
  wifi:
    interface: wlp0s20f3
    ssid: test
    key: test
